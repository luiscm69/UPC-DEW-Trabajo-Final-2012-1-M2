<h1>Listing vaccines</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Patient</th>
    <th>Owner</th>
    <th>Phone</th>
    <th>Address</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vaccines.each do |vaccine| %>
  <tr>
    <td><%= vaccine.name %></td>
    <td><%= vaccine.patient %></td>
    <td><%= vaccine.owner %></td>
    <td><%= vaccine.phone %></td>
    <td><%= vaccine.address %></td>
    <td><%= vaccine.date %></td>
    <td><%= link_to 'Show', vaccine %></td>
    <td><%= link_to 'Edit', edit_vaccine_path(vaccine) %></td>
    <td><%= link_to 'Destroy', vaccine, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vaccine', new_vaccine_path %>
