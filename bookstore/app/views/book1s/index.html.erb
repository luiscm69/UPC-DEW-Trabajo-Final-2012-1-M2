<h1>Listing book1s</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @book1s.each do |book1| %>
  <tr>
    <td><%= book1.title %></td>
    <td><%= link_to 'Show', book1 %></td>
